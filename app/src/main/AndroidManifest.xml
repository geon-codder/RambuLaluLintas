<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.geco.rambulalulintas">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/logopolantas"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logopolantas"
        android:supportsRtl="true"
        android:theme="@style/Theme.RambuLaluLintas"
        tools:targetApi="31">
        <activity
            android:name=".ujian.ResultActivity"
            android:exported="false" />
        <activity
            android:name=".ujian.UjianActivity"
            android:exported="false" />
        <activity
            android:name=".rambu.RambuActivity"
            android:exported="false" />
        <activity
            android:name=".profil.ProfilActivity"
            android:exported="false" />
        <activity
            android:name=".aturan.AturanActivity"
            android:exported="false" />
        <activity
            android:name=".SplashScreenActivity"
            android:exported="true"
            android:theme="@style/Theme.Transparent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true" />
    </application>

</manifest>